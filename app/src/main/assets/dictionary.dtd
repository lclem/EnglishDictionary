<?xml version="1.0" encoding="UTF-8"?>



<!-- DTD Fragment for Citations		-->

<!ENTITY % Cites SYSTEM 'cites.ent'> %Cites; 

<!-- Labels -->
<!ENTITY % Labels	"provLabel | regLabel | subjLabel" >
<!-- typographic tags -->
<!ENTITY % typog	"i | b | bi | sc | in | ini | su | sui | u | fr | r | render" >
<!-- status attribute -->
<!ENTITY % status "status (Active | Suppressed | Deleted) 'Active'">
<!-- url attribute (uid / link target) -->
<!ENTITY % url 'url CDATA "0"'>
<!-- refurl attribute (link pointer) -->
<!ENTITY % refurl 'refurl CDATA "0"'>
<!-- matchString contains word without syllabic marks, for searching -->
<!ENTITY % matchString 'matchString CDATA #IMPLIED'>

<!-- ========== Call special character entities ========== -->
<!ENTITY % OUPSpecChars SYSTEM 'oup-character-entities.ent'> %OUPSpecChars;




<!ELEMENT MS (#PCDATA | meta | pronGrp | gramGrp | infGrp | formGrp | 
varGrp | exGrp | ex | xrefGrp | pr | provLabel | regLabel | subjLabel | 
def | date | techGrp | etym | render | i | b | bi | sc | in | ini | su | 
sui | u | fr)*>

<!ATTLIST MS

	core (yes|no) "no"

	url NMTOKEN "0"

>

<!--Name. Usually renders: italic.-->

<!ELEMENT Name (#PCDATA)>

<!--Alternate (variant) Name. Usually renders: bold.-->

<!ELEMENT altName (#PCDATA | i)*>

<!--Global declaration for annotation element-->

<!ELEMENT annotation (#PCDATA)>

<!--bold font shift-->

<!ELEMENT b (#PCDATA)>

<!--bold italic font shift-->

<!ELEMENT bi (#PCDATA)>

<!--Biographical Label. Usually renders: Hidden.-->

<!ELEMENT bioMarker (#PCDATA)>

<!--(Cited) Work. Usually renders: according to highlight attribute 
value. true=bold; false=roman.-->

<!ELEMENT cite (#PCDATA | fr | su)*>

<!--highlight: highlight attribute allows emphasis of elements such as 
biographical lemmas and cited works to be toggled on or off.-->

<!ATTLIST cite

	highlight CDATA #IMPLIED

>

<!ELEMENT com (#PCDATA | i)*>

<!--Date. Usually renders: roman.-->

<!ELEMENT date (#PCDATA | lang | sc | i)*>

<!--Definition.-->

<!ELEMENT def (#PCDATA | cite | provLabel | regLabel | subjLabel | l | 
lp | txn | txnBin | txnFam | txnGenus | date | Name | altName | pr | 
xrefGrp | formGrp | varGrp | exGrp | pronGrp | ex | render | i | b | bi 
| sc | in | ini | su | sui | u | fr)*>

<!ATTLIST def

	url NMTOKEN "0"

>

<!--Root element, dictionary-->

<!ELEMENT dictionary (alpha+)>

<!ATTLIST dictionary

	title CDATA #IMPLIED

	xml:space (default|preserve) "preserve"

>



<!--fraction denominator-->

<!ELEMENT dn (#PCDATA | i)*>

<!--Derivative Block. Usually renders: block, generates literal 
preceding.-->

<!ELEMENT drvBlock (sub+)>

<!--Encyclopedic Block. Usually renders: rule above and below.-->

<!ELEMENT encBlock (#PCDATA | l | cite | gramGrp | infGrp | formGrp | 
varGrp | exGrp | xrefGrp | pr | lp | date | etym | txn | txnBin | txnFam 
| txnGenus | render | i | b | bi | sc | in | ini | su | sui | u | fr)*>

<!--Encyclopedic Category. Usually renders: Hidden.-->

<!ELEMENT encCategory (#PCDATA)>

<!--Etymology. Usually renders: generate surrounding "[ORIGIN:" "]". -->

<!ELEMENT etym (#PCDATA | quote | date | ff | trans | hwtrans | lang | 
cite | gramGrp | infGrp | formGrp | varGrp | exGrp | xrefGrp | pr | Name 
| txnBin | render | i | b | bi | sc | in | ini | su | sui | u | fr)*>

<!--Etymology Block. Usually renders: generate literal (usu. 'origin') 
preceding. -->

<!ELEMENT etymBlock (#PCDATA | meta | quote | date | ff | trans | 
hwtrans | lang | cite | gramGrp | infGrp | formGrp | varGrp | exGrp | 
xrefGrp | pr | Name | txnBin | render | i | b | bi | sc | in | ini | su 
| sui | u | fr)*>

<!--Example. Usually renders: italic; vertical bar connects sequential 
examples.-->

<!ELEMENT ex (#PCDATA | render | i | b | bi | sc | in | ini | su | sui | 
u | fr | r)*>

<!ATTLIST ex

	url NMTOKEN "0"

>

<!--Example Group.-->

<!ELEMENT exGrp (#PCDATA | ex | gramGrp | formGrp | varGrp | subjLabel | 
provLabel | regLabel | render | i | b | bi | sc | in | ini | su | sui | 
u | fr)*>

<!--Form. Usually renders: bold.-->

<!ELEMENT f (#PCDATA | i | u | su | varGrp)*>

<!ATTLIST f
	
	%matchString;

>

<!--Foreign Form. Usually renders: bold; italic within Etymologies.-->

<!ELEMENT ff (#PCDATA | u | i | sc)*>

<!--Foreign Headword, may be renamed (substitution[Group]) in other 
schema. Usually renders: bold italic.-->

<!ELEMENT fhw (#PCDATA)>

<!--Foreign Inflection. Usually renders: bold italic.-->

<!ELEMENT finf (#PCDATA)>

<!--Foreign Lemma. Usually renders: bold.-->

<!ELEMENT fl (#PCDATA)>

<!--Form Group. Usually renders: generates surrounding parens-->

<!ELEMENT formGrp (#PCDATA | f | provLabel | regLabel | pronGrp | varGrp 
| syntax | render | i | b | bi | sc | in | ini | su | sui | u | fr)*>

<!--fraction-->

<!ELEMENT fr (nu, dn)>

<!--Geographic Label. Usually renders: Hidden.-->

<!ELEMENT geoLabel (#PCDATA)>

<!--Geographic Marker. Usually renders: Hidden.-->

<!ELEMENT geoMarker (#PCDATA)>

<!--Grammar Group. Usually renders: roman, generates surrounding square 
braces-->

<!ELEMENT gramGrp (#PCDATA | f | gramInfo | syntax | render | i | b | bi 
| sc | in | ini | su | sui | u | fr)*>

<!--Grammatical Information. Usually renders: roman, generates 
surrounding square braces.-->

<!ELEMENT gramInfo (#PCDATA)>

<!--Homograph Number. Usually renders: superior.-->

<!ELEMENT hm (#PCDATA)>

<!--Headword, may be renamed (substitution[Group]) in other schema. 
Usually renders: bold.-->

<!ELEMENT hw (#PCDATA | r | in)*>

<!ATTLIST hw

	unit CDATA #IMPLIED
	
	%matchString;

>

<!--Headword Group. Contains headword and related information: 
pronunciation(s), labels, variants, etc.-->

<!ELEMENT hwGrp ((hw | fhw), hm?, (pronGrp | varGrp | gramGrp | infGrp | 
formGrp | xrefGrp | provLabel | regLabel | subjLabel | ps)*)>

<!--Headword Translation. Usually renders: roman, generate surrounding 
single quotes.-->

<!ELEMENT hwtrans (#PCDATA)>

<!--italic font shift-->

<!ELEMENT i (#PCDATA)>

<!-- roman font shift-->

<!ELEMENT r (#PCDATA)>

<!--inferior font shift-->

<!ELEMENT in (#PCDATA | i)*>

<!--Inflection. Usually renders: bold.-->

<!ELEMENT inf (#PCDATA)>

<!ATTLIST inf
	
	%matchString;

>

<!--Inflection Group. Usually renders: generates surrounding parens-->

<!ELEMENT infGrp (#PCDATA | inf | syntax | subjLabel | pronGrp | 
provLabel | regLabel | f | render | i | b | bi | sc | in | ini | su | 
sui | u | fr)*>

<!--inferior italic font shift-->

<!ELEMENT ini (#PCDATA)>

<!--Lemma. Usually renders: bold.-->

<!ELEMENT l (#PCDATA | varGrp | r | i | in)*>

<!ATTLIST l
	
	%matchString;

>

<!--Language. Usually renders: roman.-->

<!ELEMENT lang (#PCDATA)>

<!--Biographical Lemma. Usually renders: according to highlight 
attribute value. true=bold; false=roman.-->

<!ELEMENT lp (#PCDATA)>

<!--highlight: highlight attribute allows emphasis of elements such as 
biographical lemmas and cited works to be toggled on or off.-->

<!ATTLIST lp

	highlight CDATA #IMPLIED

>

<!--Meta. This element contains non-printing data such as comments, 
regional labels, typographic labels, bio and geo labels, etc. Goes at 
beginning of entry, sense, or block it applies to.-->

<!ELEMENT meta (annotation*, specSubj*, bioMarker*, geoMarker*, 
geoLabel*, encCategory*, typGroup*, supg*, csg*)>

<!-- superordinate group element from NODE -->

<!ELEMENT supg (sup+)>

<!-- superordinate element from NODE -->

<!ELEMENT sup (#PCDATA)>

<!-- core sense group from NODE -->

<!ELEMENT csg (#PCDATA)>

<!--fraction numerator-->

<!ELEMENT nu (#PCDATA | i)*>

<!--Paragraph.-->

<!ELEMENT para (#PCDATA | exGrp | ex | xrefGrp | pronGrp | txnGenus | 
render | i | b | bi | sc | in | ini | su | sui | u | fr)*>

<!--Phrase Block. Usually renders: block, generates literal 
preceding.-->

<!ELEMENT phrBlock (sub+)>

<!--Pronunciation. Type attribute distinguishes US, UK, IPA, etc. 
Usually renders: roman, IPA font for those types.-->

<!ELEMENT pr (#PCDATA | sc | i | u)*>

<!ATTLIST pr

	type (US | US_IPA | UK_IPA | IPA) #IMPLIED

	url NMTOKEN "0"

>

<!--Prelim. Text which applies to all senses within a sense block.-->

<!ELEMENT prelim (#PCDATA | ps | infGrp | xrefGrp | com | regLabel | 
subjLabel | gramGrp | formGrp | pronGrp | provLabel | varGrp)*>

<!--Pronunciation Group. Usually renders: generates surrounding "/" or 
"|". Contains various pronunciations.-->

<!ELEMENT pronGrp (#PCDATA | pr)*>

<!--Provenance Label. Usually renders: roman condensed.-->

<!ELEMENT provLabel (#PCDATA | r)*>

<!--Part of Speech. Usually renders: sans-serif, narrow.-->

<!ELEMENT ps (#PCDATA)>

<!--Phrasal Verb Block. Usually renders: block, generates literal 
preceding.-->

<!ELEMENT pvBlock (sub+)>

<!--Quote. Usually renders: generate surrounding single quotes.-->

<!ELEMENT quote (#PCDATA)>

<!--Regional Label. Usually renders: sans-seri narrow; hidden within 
&lt;meta&gt;.-->

<!ELEMENT regLabel (#PCDATA | r)*>

<!--Render. Special character formatting element. Usually renders: 
according to attribute.-->

<!ELEMENT render (#PCDATA)>

<!ATTLIST render	as	(italic|bold|bolditalic|smallcaps|roman|superior|inferior|sans-serif) "italic">

<!--small caps font shift-->

<!ELEMENT sc (#PCDATA | b)*>

<!--Sense. Usually renders: unnumbered if only one in entry, numbered 
otherwise. First sense runs in and generates preceding bullet.-->

<!ELEMENT sense (#PCDATA | pronGrp | meta | ps | gramGrp | infGrp | 
formGrp | varGrp | exGrp | xrefGrp | pr | provLabel | regLabel | 
subjLabel | txnGenus | MS | def | techGrp | l | ex | com | specUse | 
etym | subsense | render | i | b | bi | sc | in | ini | su | sui | u | 
fr | encBlock | QuotPara)*>

<!ATTLIST sense

	url NMTOKEN "0"

	status (Active | Suppressed | Deleted) 'Active'

>



<!--Sense Block. Contains sense(s).-->

<!ELEMENT senseBlock (meta?, prelim?, sense+)>

<!ATTLIST senseBlock

	url NMTOKEN "0"

>

<!ELEMENT sn (#PCDATA)>

<!--Specialist Subject. Usually renders: Hidden.-->

<!ELEMENT specSubj (#PCDATA)>

<!ELEMENT specUse (#PCDATA | MS | xrefGrp | ex | render | i | b | bi | 
sc | in | ini | su | sui | u | fr)*>

<!--superior font shift-->

<!ELEMENT su (#PCDATA | i)*>

<!--Sub Entry. Usually renders: new line.-->

<!ELEMENT sub (#PCDATA | meta |  l  | ps | gramGrp | pronGrp | infGrp | 
formGrp | varGrp | exGrp | xrefGrp | provLabel | regLabel | subjLabel | 
pr | def | etym | MS | specUse | ex | sense | render | i | b | bi | sc | 
in | ini | su | sui | u | fr | ehw)*>

<!-- ehw for location in electronic pubs -->

<!ELEMENT ehw (#PCDATA | x)*>

<!ATTLIST ehw

	type CDATA #IMPLIED

>

<!--Subject Label. Usually renders: sans-serif narrow.-->

<!ELEMENT subjLabel (#PCDATA | r)*>

<!--Sub Sense. Usually renders: alphabetical counter.-->

<!ELEMENT subsense (#PCDATA | ps | exGrp | ex | varGrp | provLabel | 
regLabel | subjLabel | def | xrefGrp | formGrp | gramGrp | render | i | 
b | bi | sc | in | ini | su | sui | u | fr | QuotPara)*>

<!--superior italic font shift-->

<!ELEMENT sui (#PCDATA)>

<!--Syntax.-->

<!ELEMENT syntax (#PCDATA)>

<!--Technical Information Group. Usually renders: roman; generates 
preceding bullet-->

<!ELEMENT techGrp (#PCDATA | techMS | varGrp | xrefGrp | txn | txnBin | 
txnFam | txnGenus | l | render | i | b | bi | sc | in | ini | su | sui | 
u | fr)*>

<!--Technical Meaning Statement. Usually renders: roman; generates 
preceding bullet-->

<!ELEMENT techMS (#PCDATA | def | gramGrp | varGrp | xrefGrp | provLabel 
| txn | txnBin | txnFam | txnGenus | i | b | bi | sc | in | ini | su | 
sui | u | fr)*>

<!--Translation. Usually renders: roman, generate surrounding single 
quotes.-->

<!ELEMENT trans (#PCDATA | Name | u | i)*>

<!--Taxonomic info. Usually renders: according to highlight attribute 
value. true=bold; false=roman.-->

<!ELEMENT txn (#PCDATA | in)*>

<!--highlight: highlight attribute allows emphasis of elements such as 
biographical lemmas and cited works to be toggled on or off.-->

<!ATTLIST txn

	highlight CDATA #IMPLIED

>

<!--Taxonomic Binomial. Usually renders: italic.-->

<!ELEMENT txnBin (#PCDATA | varGrp | r)*>

<!--Taxonomic Family. Usually renders: roman.-->

<!ELEMENT txnFam (#PCDATA)>

<!--Taxonomic Genus. Usually renders: roman.-->

<!ELEMENT txnGenus (#PCDATA | varGrp)*>

<!--Typographic Group. Usually renders: Hidden.-->

<!ELEMENT typGroup (#PCDATA)>

<!--underscore-->

<!ELEMENT u (#PCDATA)>

<!--Usage Note. Usually renders: rule above and below, possible usage 
literal.-->

<!ELEMENT usage (meta?, para+)>

<!ATTLIST usage

	url NMTOKEN "0"

	status (Active | Suppressed | Deleted) 'Active'

>

<!--Variant. Usually renders: bold; is roman or italic within 
&lt;txnBin&gt; or &lt;txnGenus&gt; according to highlight attribute 
value. true=bold; false=roman.-->

<!ELEMENT v (#PCDATA | i | b | r | in | su)*>

<!--highlight: highlight attribute allows emphasis of elements such as 
biographical lemmas and cited works to be toggled on or off.-->

<!ATTLIST v

	highlight CDATA #IMPLIED
	
	%matchString;

>

<!--Variant Group. Usually renders: generate surrounding parens-->

<!ELEMENT varGrp (#PCDATA | v | pronGrp | provLabel | subjLabel | 
regLabel | syntax | infGrp | render | i | b | bi | sc | in | ini | su | 
sui | u | fr)*>

<!--Cross reference target. Usually renders: bold small caps.-->

<!ELEMENT x (#PCDATA | in)*>

<!--Cross reference container. Holds term, homograph number, part of 
speech, sense number.-->

<!ELEMENT xref (#PCDATA | x | ps | hm | sn)*>

<!ATTLIST xref

	refurl NMTOKENS #IMPLIED

	type (entry | sense | lemma | usage) #IMPLIED

>

<!--Cross-Reference Group.-->

<!ELEMENT xrefGrp (#PCDATA | xref | pronGrp | ps | xu | sn | subjLabel | 
provLabel | regLabel | render | i | b | bi | sc | in | ini | su | sui | 
u | fr)*>

<!-- temp element until xrefs are done. marks xrefs to usage notes -->

<!ELEMENT xu (#PCDATA)>

<!--Alphabetical container consists of entries.-->

<!ELEMENT alpha (entry)+>

<!ATTLIST alpha

	letter CDATA #REQUIRED

>

<!--Entry Data Type-->

<!ELEMENT entry (meta?, hwGrp, senseBlock+, encBlock?, phrBlock?, 
pvBlock?, drvBlock?, etymBlock?, usage?, specialFeature?, wordlist?, figure*)>

<!ATTLIST entry

	title CDATA #IMPLIED

	url NMTOKEN "0"

	status (Active | Suppressed | Deleted) "Active"

	entry CDATA #REQUIRED

	sortkey CDATA #REQUIRED

	type CDATA #IMPLIED

	stage CDATA #IMPLIED

>

<!-- ============== Word lists ============== -->
<!ELEMENT wordlist	(CompInstructs?, lhw, listings, xrefGrp?, ln*) >

<!-- composition instructions					-->
<!ELEMENT CompInstructs	(#PCDATA) >
<!-- list headword						-->
<!ELEMENT lhw		(#PCDATA) >
<!-- listings							-->
<!ELEMENT listings	(lcol+) >
<!-- list column						-->
<!ELEMENT lcol		(#PCDATA | li | lip | lic | lh | %Labels; | xrefGrp)* >
<!-- list head							-->
<!ELEMENT lh		(#PCDATA | render)* >
<!-- list item							-->
<!ELEMENT li		(#PCDATA | render)* >
<!-- list in phrase/tradmark?					-->
<!ELEMENT lip		(#PCDATA | render)* >
<!-- list item compound						-->
<!ELEMENT lic		(#PCDATA | render)* >
<!-- list note							-->
<!ELEMENT ln		(#PCDATA | render)* >


<!-- ============== Figures ============== -->
<!ELEMENT figure (caption?) >
<!ATTLIST figure
		%url;
		%refurl;
		%status;
		resource CDATA #IMPLIED
		height CDATA #IMPLIED
		width CDATA #IMPLIED
		units (cols|pt|in) "cols" >
<!ELEMENT caption (#PCDATA | %typog;)* >


<!-- ============== Special Features ============== -->
<!ELEMENT specialFeature	(head?, body) >
<!ATTLIST specialFeature	type	(awkward_synonyms | confusables | word_banks | word_continuums | word_notes | usage_notes | list_words) #REQUIRED 
				%url; >

<!ELEMENT body	(p | list | head)* >

<!ELEMENT list	(head?, (item+ | (label, item)+)) >
<!ATTLIST list 	type (ordered | bulleted | gloss | simple | listwords) "simple" >

<!ELEMENT head	(#PCDATA | render)* >

<!ELEMENT p	(#PCDATA | render | ex)* >

<!ELEMENT item	(#PCDATA | render | ex | l  | ps | inf | %Labels;)* >

<!ELEMENT label	(#PCDATA | render)* >







